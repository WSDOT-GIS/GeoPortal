    "wim": {
      "label": "PTR (WIM)",
      "url": "https://data.wsdot.wa.gov/arcgis/rest/services/Traffic/PTRSites/MapServer/0",
      "query": {
        "where": "1 = 1",
        "returnGeometry": true,
        "outFields": [ "ADCTrafficSDE.DBO.PTRSites.SiteID" ]
      }
    },
    "noWim": {
      "label": "PTR (non-WIM)",
      "url": "https://data.wsdot.wa.gov/arcgis/rest/services/Traffic/PTRSites/MapServer/1",
      "query": {
