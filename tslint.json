{
    "defaultSeverity": "warn",
    "extends": [
        "tslint:recommended",
        "tslint-config-prettier",
        "tslint-plugin-prettier"
    ],
    "jsRules": {},
    "rules": {
        "prettier": true,
        "object-literal-key-quotes": false,
        "object-literal-sort-keys": false,
        "trailing-comma": false,
        "arrow-parens": false
    }
}